$("#how").val("why?");
$("#mood_val").val('');
$(".smiley-selected").toggleClass("smiley-selected");
var series = chart.series[0];
var shift = series.data.length > 9;
var ts = new Date("<%=@mood.report_time%>");
var mood = {"val":<%=@mood.mood%>,"desc":"<%=@mood.desc%>","time":month[ts.getMonth()]+" "+ts.getDate(),"date":ts.toString()};
var p = {"name":mood.desc,"y":mood.val};
series.addPoint(p,false,shift);
var categories = chart.xAxis[0].categories;
categories.push(mood.time);
if(shift){
  categories.shift();
}
chart.xAxis[0].setCategories(categories, false);
chart.redraw();

